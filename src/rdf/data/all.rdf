  <ns1:Keyword rdf:about="https://kunshujo.dl.itc.u-tokyo.ac.jp/api/keyword/輿">
    <ns2:associatedMedia rdf:resource="https://upload.wikimedia.org/wikipedia/commons/3/3e/Aoi-Matsuri-01.jpg"/>
    <ns2:image rdf:resource="https://upload.wikimedia.org/wikipedia/commons/thumb/3/3e/Aoi-Matsuri-01.jpg/200px-Aoi-Matsuri-01.jpg"/>
