    <ns2:image rdf:resource="https://upload.wikimedia.org/wikipedia/commons/thumb/7/7a/Kashima-jingu_haiden-1.JPG/200px-Kashima-jingu_haiden-1.JPG"/>
    <ns3:sameAs rdf:resource="http://ja.dbpedia.org/resource/鹿島神宮"/>
