    <ns2:associatedMedia rdf:resource="https://upload.wikimedia.org/wikipedia/commons/c/c0/Uesugi_Kagekatsu.jpg"/>
    <ns2:image rdf:resource="https://upload.wikimedia.org/wikipedia/commons/thumb/c/c0/Uesugi_Kagekatsu.jpg/200px-Uesugi_Kagekatsu.jpg"/>
