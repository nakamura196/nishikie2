  <ns1:Keyword rdf:about="https://kunshujo.dl.itc.u-tokyo.ac.jp/api/keyword/八百屋">
    <ns2:associatedMedia rdf:resource="https://upload.wikimedia.org/wikipedia/commons/b/be/%E9%9D%92%E6%9E%9C%E5%BA%97%E5%8C%97%E4%BA%AC%E9%A7%85%E5%89%8D.jpg"/>
    <ns2:image rdf:resource="https://upload.wikimedia.org/wikipedia/commons/thumb/b/be/%E9%9D%92%E6%9E%9C%E5%BA%97%E5%8C%97%E4%BA%AC%E9%A7%85%E5%89%8D.jpg/200px-%E9%9D%92%E6%9E%9C%E5%BA%97%E5%8C%97%E4%BA%AC%E9%A7%85%E5%89%8D.jpg"/>
    <ns3:sameAs rdf:resource="http://ja.dbpedia.org/resource/八百屋"/>
    <rdfs:label>八百屋</rdfs:label>
    <ns2:description xml:lang="ja">八百屋（やおや）は、野菜や果物を主に販売する店の日本での呼称。「青果店」（せいかてん）とも呼ばれる。</ns2:description>
